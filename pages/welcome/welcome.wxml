<!-- pages/welcome.wxml -->
<!-- wexml是编写小程序骨架的文件 -->
<!-- div和view是一样的 -->
<view class='container'>
    <image class='user-avatar' src="/images/avatar/4.png"></image>
    <text class='user-name'>
        <text >Hello</text>,七月</text>
    <view class='moto-container' bindtap="onTap">
        <text class='moto' >开启小程序之旅</text>
    </view>
</view>